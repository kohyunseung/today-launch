// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Chat {
  date      String  @id
  message   String
  createdAt DateTime @default(now())
}

model Menu {
  date      String  @id
  menu      String
  createdAt DateTime @default(now())
}